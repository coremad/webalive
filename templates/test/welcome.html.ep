% layout 'default';
% title 'Welcome';
<h2><%= $msg %></h2>
<p>
  This page was generated from the template "templates/example/welcome.html.ep"
  and the layout "templates/layouts/default.html.ep",
  <%= link_to 'click here' => url_for %> to reload the page or
  <%= link_to 'here' => '/index.html' %> to move forward to a static page.
</p>
<div>
  <button id="rbutton">Refresh</button><span id="rtime"></span>
  <div>
    <form action="api/add/" method="post" id="addform">
      <input name="url" id="url" type="text" placeholder="http://">
      <input type="submit" value="Add" id="addurl">
    </form>
  </div>
</div>
<span id="err"></span>
<div>
  <table id="here_table"></table>
</div>

